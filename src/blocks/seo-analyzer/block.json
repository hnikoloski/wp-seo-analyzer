{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "name": "wp-seo-analyzer/seo-analyzer",
    "version": "1.0.0",
    "title": "SEO Analyzer",
    "category": "widgets",
    "icon": "chart-bar",
    "description": "Analyze SEO metrics for your posts",
    "keywords": ["seo", "analyzer", "content", "keywords"],
    "supports": {
        "html": false,
        "align": ["wide", "full"]
    },
    "textdomain": "wp-seo-analyzer",
    "viewScript": "file:../../../build/index.js",
    "editorScript": "file:../../../build/index.js",
    "style": "file:../../../build/style-index.css",
    "attributes": {
        "align": {
            "type": "string",
            "default": "wide"
        }
    }
}
